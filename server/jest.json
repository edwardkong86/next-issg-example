{
	"testEnvironment": "node",
	"verbose": false,
	"clearMocks": true,
	"modulePaths": ["<rootDir>", "node_modules"],
	"setupFilesAfterEnv": ["<rootDir>/helpers/setupTests.js"],
	"transform": {
		"^.+\\.(js)$": "babel-jest"
	},
	"globalSetup": "<rootDir>/helpers/globalSetup.js",
	"globalTeardown": "<rootDir>/helpers/teardownDB.js",
	"moduleFileExtensions": ["js", "json"]
}
